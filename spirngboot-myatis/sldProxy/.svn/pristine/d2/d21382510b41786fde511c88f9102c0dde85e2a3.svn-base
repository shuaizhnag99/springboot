package com.ule.uhj.sldProxy.util;

public class RequestContextBean {
	private Integer type; //操作类型【1:获取短信信息、2：获取通信录信息、3：获取设备定位信息、4：获取通话记录信息】
	private Object data; //请求参数正文
	public Integer getType() {
		return type;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	public Object getData() {
		return data;
	}
	public void setData(Object data) {
		this.data = data;
	}
	public RequestContextBean() {
		super();
	}
	public RequestContextBean(Integer type, Object[] data) {
		super();
		this.type = type;
		this.data = data;
	}
}
