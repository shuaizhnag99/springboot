<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.ule.if.vpsLoan</groupId>
	<artifactId>applyTransact-ejb-api</artifactId>
	<version>${env}-1.1.0.${scmVersion}</version>
	<packaging>jar</packaging>
	<name>applyTransact-ejb-api</name>
	<description>the api of applyTransact-ejb-service</description>
	<scm>
		<connection>scm:svn:https://172.24.144.104/svn/eachnet/trunk/PostMall/ule.uhj/ule.if/applyTransact-ejb-api</connection>
		<developerConnection>scm:svn:https://172.24.144.104/svn/eachnet/trunk/PostMall/ule.uhj/ule.if/applyTransact-ejb-api</developerConnection>
		<url>https://172.24.144.104/svn/eachnet/trunk/PostMall/ule.uhj/ule.if/applyTransact-ejb-api</url>
	</scm>
	<properties>
		<env>beta</env>
		<scmVersion>697488</scmVersion>
	</properties>
	<dependencies>
	<dependency>
            <groupId>com.ule.lab.wildfly</groupId>
            <artifactId>wildfly-basic-client</artifactId>
            <version>${env}-1.0.0</version>
            <scope>provided</scope>
        </dependency>
	<!-- 	<dependency>
			<groupId>com.ule.jboss</groupId>
			<artifactId>HAJNDIContext</artifactId>
			<version>1.0.1</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>jbossClient</groupId>
			<artifactId>jbossClient</artifactId>
			<version>5.1.GA</version>
			<scope>provided</scope>
		</dependency> -->
		<dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>1.2.14</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.sf.json</groupId>
			<artifactId>json</artifactId>
			<version>2.2.3</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>commons-lang</groupId>
			<artifactId>commons-lang</artifactId>
			<version>2.5</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>commons-collections</groupId>
			<artifactId>commons-collections</artifactId>
			<version>3.1</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>commons-beanutils</groupId>
			<artifactId>commons-beanutils</artifactId>
			<version>1.8.3</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.sf.ezmorph</groupId>
			<artifactId>ezmorph</artifactId>
			<version>1.0.6</version>
			<scope>provided</scope>
		</dependency>
	</dependencies>
	<repositories>
		<repository>
			<releases>
				<enabled>true</enabled>
				<updatePolicy>always</updatePolicy>
				<checksumPolicy>fail</checksumPolicy>
			</releases>
			<id>dev-central</id>
			<name>Ule Dev Dependent Repository</name>
			<layout>default</layout>
			<url>http://maven.${env}.ulechina.tom.com/artifactory/repo</url>
			<snapshots>
				<updatePolicy>always</updatePolicy>
				<checksumPolicy>warn</checksumPolicy>
			</snapshots>
		</repository>
	</repositories>

	<distributionManagement>
		<repository>
			<id>tom-maven-repository</id>
			<name>Tom Maven Repository</name>
			<url>http://maven.${env}.ulechina.tom.com/artifactory/libs-releases-local</url>
		</repository>
	</distributionManagement>

	<build>
		<resources>
			<resource>
				<directory>src/main/resources/${env}</directory>
				<includes>
					<include>*.properties</include>
				</includes>
			</resource>
			<!--<resource>
				<directory>src/main/resources/common</directory>
				<includes>
					<include>*.properties</include>
				</includes>
			</resource>-->
		</resources>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.3</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
		</plugins>
	</build>
	
</project>